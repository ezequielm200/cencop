<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('./partials/head') %>
        <title>Usuarios</title>
    </head>
    <body>
    <%- include('./partials/header') %>
        <main>
            <h3><%= title %></h3>
            <div class="userList">
            <% for(i=0;i<users.length;i++) { %>
                <div class="user">
                    <img src="/images/profileImages/<%=users[i].avatar%>" alt="" />
                                    <P class="id">ID: <%=users[i].id%></P>
                                    <P>Nombre: <%=users[i].nombre%></P>
                                    <P>Apellidos: <%=users[i].apellido%></P>
                                    <P>email: <%=users[i].email%></P>
                                </div>

                                               <% } %>
                                            </div>
        </main>
        <%- include('./partials/footer') %> 

    </body>
</html>

